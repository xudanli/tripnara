{
  "name": "tripmind-backend",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "npx nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "npx nest start",
    "start:dev": "npx nest start --watch",
    "start:debug": "npx nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "llm:deepseek": "NODE_ENV=test ts-node --transpile-only scripts/deepseek-test.ts",
    "db:init": "ts-node --transpile-only scripts/init-db.ts",
    "db:test": "ts-node --transpile-only scripts/test-db-connection.ts",
    "test:api": "ts-node --transpile-only scripts/test-api-endpoints.ts",
    "test:itinerary-crud": "ts-node --transpile-only scripts/test-itinerary-crud.ts",
    "test:redis": "ts-node --transpile-only scripts/test-redis.ts",
    "test:redis:integration": "ts-node --transpile-only scripts/test-redis-integration.ts",
    "view:queue:jobs": "ts-node --transpile-only scripts/view-queue-jobs.ts",
    "diagnose:google-oauth": "ts-node --transpile-only scripts/diagnose-google-oauth-network.ts",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js -d typeorm.config.ts",
    "migration:run": "npm run typeorm -- migration:run",
    "migration:revert": "npm run typeorm -- migration:revert",
    "migration:generate": "npm run typeorm -- migration:generate",
    "seed:currency": "ts-node --transpile-only scripts/seed-currency-data.ts",
    "generate:country-mappings": "ts-node --transpile-only scripts/generate-country-mappings.ts",
    "import:country-mappings": "ts-node --transpile-only scripts/import-country-mappings.ts",
    "generate:all-currencies": "ts-node --transpile-only scripts/generate-all-currencies.ts",
    "import:all-currencies": "ts-node --transpile-only scripts/import-all-currencies.ts",
    "generate:countries": "ts-node --transpile-only scripts/generate-countries.ts",
    "import:countries": "ts-node --transpile-only scripts/import-countries.ts",
    "cleanup:orphan-data": "ts-node --transpile-only scripts/cleanup-orphan-data.ts"
  },
  "dependencies": {
    "@google/genai": "^1.30.0",
    "@nestjs/axios": "^4.0.1",
    "@nestjs/bull": "^11.0.4",
    "@nestjs/common": "^11.0.1",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.0.1",
    "@nestjs/jwt": "^11.0.1",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.0.1",
    "@nestjs/swagger": "^11.2.1",
    "@nestjs/terminus": "^11.0.0",
    "@nestjs/typeorm": "^11.0.0",
    "@types/nodemailer": "^7.0.4",
    "axios": "^1.13.2",
    "bullmq": "^5.65.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "cookie-parser": "^1.4.7",
    "google-auth-library": "^10.5.0",
    "https-proxy-agent": "^7.0.6",
    "ioredis": "^5.8.2",
    "nodemailer": "^7.0.11",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.16.3",
    "pino": "^10.1.0",
    "pino-pretty": "^13.1.2",
    "prom-client": "^15.1.3",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "typeorm": "^0.3.27",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.18.0",
    "@nestjs/cli": "^11.0.0",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.0.1",
    "@types/express": "^5.0.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^22.10.7",
    "@types/passport-jwt": "^4.0.1",
    "@types/supertest": "^6.0.3",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.2",
    "globals": "^16.0.0",
    "jest": "^30.2.0",
    "prettier": "^3.4.2",
    "source-map-support": "^0.5.21",
    "sql.js": "^1.13.0",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.5",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.20.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "overrides": {
    "ansis": "1.5.1"
  }
}
